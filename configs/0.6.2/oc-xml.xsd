                    <xs:sequence>
                        <xs:element name="AdviseWindows" type="xs:boolean"/>
                        <xs:element name="MLB" type="xs:string"/>
                        <xs:element name="ProcessorType" type="xs:nonNegativeInteger"/>
                        <xs:element name="ROM" type="xs:base64Binary"/>
                        <xs:element name="SpoofVendor" type="xs:boolean"/>
